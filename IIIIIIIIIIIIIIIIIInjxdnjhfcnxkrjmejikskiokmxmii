local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Humanoid")
Section:NewSlider("Walkspeed", "Sets your walkspeed", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Section:NewSlider("Jumppower", "Sets your jumppower", 500, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
local Tab = Window:NewTab("Keybind")
local Section = Tab:NewSection("Keybind button")
Section:NewKeybind("Keybind:", "The keybind to this script", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
local Tab = Window:NewTab("Currency")
local Section = Tab:NewSection("Arm bucks")
Section:NewSlider("Arm bucks", "Sets how much armbucks you'll get", -10000000, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    local args = {
    [1] = s,
    [2] = 0
}
game:GetService("ReplicatedStorage"):WaitForChild("bicep"):FireServer(unpack(args))
end)
